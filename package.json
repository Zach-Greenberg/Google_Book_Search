{
  "name": "google-books",
  "version": "1.0.0",
  "description": "Google Books Search",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'googlebooks/*'\" \"npm run client\"",
    "client": "cd googlebooks && npm run start",
    "install": "cd googlebooks && npm install",
    "build": "cd googlebooks && npm run build",
    "heroku-postbuild": "cd googlebooks && npm install --only=dev && npm install && npm run build"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "express": "^4.16.3",
    "mongoose": "^5.3.16",
    "if-env": "^1.0.4"
  }
}
